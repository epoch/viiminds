<h1>Listing post</h1>

<%= link_to 'new post', new_post_path %>

<% @posts.each do |post| %>
  <h2>
    <%= link_to post.title, edit_post_path(post) %>
    <%= link_to 'show', post_path(post) %>
    <%= link_to 'destroy', post_path(post), method: :delete, class: 'dangerous', data: { confirm: 'Really?' } %>
  </h2>
<% end %>